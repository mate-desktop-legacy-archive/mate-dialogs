msgid ""
"Specifies the file or directory that is selected in the file selection "
"dialog when the dialog is first shown."
msgstr ""

#: C/matedialog.xml:561(option)
msgid "--multiple"
msgstr "--multiple"

#: C/matedialog.xml:563(para)
msgid ""
"Allows the selection of multiple filenames in the file selection dialog."
msgstr ""

#: C/matedialog.xml:568(option)
msgid "--directory"
msgstr "--directory"

#: C/matedialog.xml:570(para)
msgid "Allows only selection of directories in the file selection dialog."
msgstr ""

#: C/matedialog.xml:575(option)
msgid "--save"
msgstr "--save"

#: C/matedialog.xml:577(para)
msgid "Set the file selection dialog into save mode."
msgstr ""

#: C/matedialog.xml:582(term) C/matedialog.xml:727(term)
msgid "<option>--separator</option>=<replaceable>separator</replaceable>"
msgstr ""

#: C/matedialog.xml:584(para)
msgid ""
"Specifies the string that is used to divide the returned list of filenames."
msgstr ""

