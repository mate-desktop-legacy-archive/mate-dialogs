"        matedialog --progress \\\n"
"          --title=\"Update System Logs\" \\\n"
"          --text=\"Scanning mail logs...\" \\\n"
"          --percentage=0\n"
"\n"
"        if [ \"$?\" = -1 ] ; then\n"
"                matedialog --error \\\n"
"                  --text=\"Update canceled.\"\n"
"        fi\n"
"\n"
